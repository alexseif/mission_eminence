{% extends 'admin/base.html.twig' %}
{% import 'macros/_links.html.twig' as links %}

{% block h1 %}MMM
{% endblock %}
{% block cardActions %}
	<div>
		{{ links.back('admin_mmm_index') }}
		{{ links.edit('admin_mmm_edit', mmm.id) }}
		{{ include('admin/mmm/_delete_form.html.twig') }}
	</div>
{% endblock %}

{% block body %}
	<table class="table">
		<tbody>
			<tr>
				<th>Title</th>
				<td>{{ mmm.title }}</td>
			</tr>
			<tr>
				<th>Content</th>
				<td>{{ mmm.content }}</td>
			</tr>
			<tr>
				<th>Image</th>
				<td>{{ mmm.image }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ mmm.createdAt ? mmm.createdAt|date : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ mmm.updatedAt ? mmm.updatedAt|date : '' }}</td>
			</tr>
		</tbody>
	</table>
{% endblock %}
